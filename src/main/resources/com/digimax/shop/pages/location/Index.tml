<html t:type="layout" title="Contact shop.digimax.com"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">
    <span>
        <t:if test="receiving">
            <span t:type="domain/receivingui" receiving="location"/>
        </t:if>
        <t:if test="shelf">
            <span t:type="domain/shelfui" shelf="location"/>
        </t:if>
        <t:if test="shipping">
            <span t:type="domain/shippingui" shipping="location"/>
        </t:if>
        <t:if test="store">
            <span t:type="domain/storeui" store="location"/>
        </t:if>
        <t:if test="warehouse">
            <span t:type="domain/warehouseui" warehouse="location"/>
        </t:if>
    </span>
</html>